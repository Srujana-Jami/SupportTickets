@page
@model SupportPagesPro.Pages.SupportTickets.DetailsModel

@{
    ViewData["Title"] = "Details";
}




<div class="container">
    <div class="row">
        <div class="col-md-8">

            <form method="post">
                <input type="hidden" asp-for="SupportTicket.Id" />
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="col">
                    <div class="form-group">
                        @Html.DisplayFor(model => model.SupportTicket.Id) -


                        @Html.DisplayFor(model => model.SupportTicket.YourMail)
                    </div>
                    </div>
                <div class="form-group">

                    @Html.DisplayFor(model => model.SupportTicket.Subject)

                </div>
                <div class="form-group">

                    @Html.DisplayFor(model => model.SupportTicket.Description)
                </div>

                    <div class="row">
                       
                        <div class="col">
                            CreatedDate: @Html.DisplayFor(model => model.SupportTicket.CreatedDate)
                        </div>
                        <div class="col text-right">

                            UpdatedDate: @Html.DisplayFor(model => model.SupportTicket.UpdatedDate)
                        </div>
                           
                    </div>

                  
</form>
    </div>


    </div>


</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}



